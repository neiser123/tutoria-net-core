@model List<tutoria_net_core.Models.Amigo>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <h1>lista de objetos</h1>

    <table>
        <tr>
            <th>id</th>
            <th>nombre</th>
            <th>email</th>
            <th>ciudad</th>

        </tr>
        <tr>
            @foreach (var amigo in Model)
            {
            <tr>
                <td>@amigo.Id    </td>
                <td>@amigo.nombre</td>
                <td>@amigo.email </td>
                <td>@amigo.ciudad</td>
            </tr>
             }

         </tr>

        </table>


</body>
</html>
